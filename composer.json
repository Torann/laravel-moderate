{
    "name": "torann/moderate",
    "description": "A simple moderation system for Laravel.",
    "keywords": ["laravel", "moderate", "moderation", "spam", "blacklist", "restrict", "security", "protect"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.1",
        "illuminate/cache": "~5.1"
    },
    "suggest": {
        "illuminate/database": "Allows for storing blacklist in the database"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Torann\\Moderate\\": "src/Torann/Moderate"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "archive" : {
        "exclude": [
            "/tests"
        ]
    }
}
