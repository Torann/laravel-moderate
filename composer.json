{
    "name": "torann/moderate",
    "description": "A simple moderation system for Laravel 4.",
    "keywords": ["laravel", "moderate", "moderation", "spam", "blacklist", "restrict"],
    "license": "BSD 2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "illuminate/database": "4.*",
        "illuminate/cache": "4.1.x"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Torann\\Moderate": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}